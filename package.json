{
  "name": "md-react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "start": "npm run set:config:dev & npm run sass && react-scripts start & npm run sass:watch",
    "build:dev": "npm run set:config:dev && PUBLIC_URL= react-scripts build",
    "build:prod": "npm run set:config:prod && PUBLIC_URL= GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "prettier": "prettier --write \"{src/!(assets),src,__tests__}/**/*.js\"",
    "set:config:dev": "CONFIG=development gulp set:config",
    "set:config:prod": "CONFIG=production gulp set:config",
    "eject": "react-scripts eject",
    "clean": "rimraf ./build",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "flag-icon-css": "^3.0.0",
    "gulp-replace": "^1.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.4",
    "react-toastify": "^4.0.1",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.0",
    "styled-components": "^3.2.6",
    "sweetalert2": "^7.20.7",
    "sweetalert2-react-content": "^1.0.0",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^4.0.1",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0"
  }
}
