{
  "name": "md-react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "start": "npm run set:config:dev & npm run sass && react-scripts start & npm run sass:watch",
    "build:dev": "npm run set:config:dev && PUBLIC_URL= react-scripts build",
    "build:prod": "npm run set:config:prod && PUBLIC_URL= GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "prettier": "prettier --write \"{src/!(assets),src,__tests__}/**/*.js\"",
    "set:config:dev": "CONFIG=development gulp set:config",
    "set:config:prod": "CONFIG=production gulp set:config",
    "eject": "react-scripts eject",
    "clean": "rimraf ./build",
    "test": "rreact-scripts test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "animate.css": "^3.7.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^5.0.1",
    "flag-icon-css": "^3.2.1",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.7.2",
    "qs": "^6.5.2",
    "react": "^16.6.0",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.7.2",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-slick": "^0.23.2",
    "react-toastify": "^4.4.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.13",
    "styled-components": "^4.0.3",
    "sweetalert2": "^7.28.11",
    "sweetalert2-react-content": "^1.0.1",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
