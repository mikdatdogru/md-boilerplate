{
  "name": "md-react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.5.0"
  },
  "scripts": {
    "start": "yarn run set:config:dev && react-scripts start",
    "build:dev": "yarn run set:config:dev && PUBLIC_URL= react-scripts build",
    "build:prod": "yarn run set:config:prod && PUBLIC_URL= GENERATE_SOURCEMAP=false react-scripts build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write \"{src/!(assets),src,__tests__}/**/*.js\"",
    "set:config:dev": "CONFIG=development node configSetter.js",
    "set:config:prod": "CONFIG=production node configSetter.js",
    "eject": "react-scripts eject",
    "clean": "rimraf ./build",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettier && yarn run lint:fix",
      "pre-push": "yarn run test"
    }
  },

  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.4.0",
    "core-js": "^3.1.3",
    "flag-icon-css": "^3.3.0",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-slick": "^0.24.0",
    "react-toastify": "^5.2.1",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "styled-components": "^4.3.1",
    "sweetalert2": "^8.12.1",
    "sweetalert2-react-content": "^1.1.0",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
